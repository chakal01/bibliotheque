= bootstrap_form_for @emplacement, layout: :horizontal do |f|
  - if @emplacement.errors.any?
    #error_explanation
      %h2= "#{pluralize(@emplacement.errors.count, "error")} prohibited this emplacement from being saved:"
      %ul
        - @emplacement.errors.full_messages.each do |msg|
          %li= msg

  = f.text_field :nom

  = f.submit 'Sauver', class: "btn"
